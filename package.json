{
  "name": "ai-parser",
  "version": "1.0.0",
  "description": "",
  "main": "dist/parser.js",
  "author": "Fran√ßois Lunette",
  "scripts": {
    "dev": "npm run dev:server && npm run dev:parser",
    "cli": "node dist/cli.js",
    "dev:parser": "webpack --watch",
    "start:server": "node dist/server.js",
    "dev:server": "webpack --config webpack.config.server.js --watch",
    "dev:client": "webpack --config webpack.config.client.js --watch",
    "test": "./node_modules/.bin/ava --verbose",
    "test:auto": "./node_modules/.bin/ava --watch --verbose",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flubox/ai-parser.git"
  },
  "bugs": {
    "url": "https://github.com/flubox/ai-parser/issues"
  },
  "homepage": "https://github.com/flubox/ai-parser#readme",
  "dependencies": {
    "aws-sdk": "^2.34.0",
    "blueimp-md5": "^2.7.0",
    "convert-css-color-name-to-hex": "^0.1.1",
    "drag-and-drop-files": "0.0.1",
    "json-loader": "^0.5.4",
    "uuid": "^3.0.1",
    "xml-js": "^1.0.2"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "browser-env": "^2.0.29",
    "browser-sync": "2.18.8",
    "browser-sync-webpack-plugin": "1.1.4",
    "esdoc": "^0.5.2",
    "eslint": "^3.16.1",
    "eslint-plugin-prefer-spread": "^1.0.3",
    "exports-loader": "^0.6.4",
    "jsdom": "^9.12.0",
    "muicss": "^0.9.12",
    "node-sass": "^4.5.0",
    "npm-watch": "^0.1.8",
    "nyc": "^10.1.2",
    "sinon": "^1.17.7",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "src/**/*.js",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "watch": {
    "test": "test/*.js"
  }
}
